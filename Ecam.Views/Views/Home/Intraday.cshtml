@{ Layout = null; }
<div class="page-header">
    <div class="col-md-12 no-padding m-b-10 border-white header p-b-10">
        <form id="frmCompanySearch" class="form-inline form-validate" role="form" onsubmit="return false">
            <div class="form-group" style="margin-left:10px; margin-right:10px;float:left;">
                <h1 class="page-title" style="margin-top:-4px;">Intraday</h1>
            </div>
            <div class="form-group m-r-10 m-t-10">
                <input type="hidden" name="symbols" />
            </div>
            <div class="form-group m-r-10 m-t-10">
                <input type="hidden" name="categories" />
            </div>
            <div class="form-group m-r-10 m-t-10">
                <input type="hidden" name="mf_ids" />
            </div>
            <div class="form-group m-r-10 m-t-10">
                <input type="text" name="from_price" class="form-control input-sm" placeholder="From Price" style="width:100px" />
            </div>
            <div class="form-group m-r-10 m-t-10">
                <input type="text" name="to_price" class="form-control input-sm" placeholder="To Price" style="width:100px" />
            </div>
            <div class="clearfix"></div>
            <div class="m-l-15">
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_book_mark" /> Show Book Mark
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_nifty_50" /> Nifty 50
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_nifty_100" /> Nifty 100
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_nifty_200" checked /> Nifty 200
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_mf" /> MF Stock
                        </label>
                    </div>
                </div>
            </div>
            <div class="clearfix"></div>
            <div class="m-l-15">
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_all_time_low" />Low Last Month
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_all_time_high" />High Last Month
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_all_time_low_15_days" /> Low 15 Days
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_all_time_high_15_days" /> High 15 Days
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_all_time_low_5_days" /> Low 5 Days
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_all_time_high_5_days" /> High 5 Days
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_all_time_low_2_days" /> Low 2 Days
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_all_time_high_2_days" /> High 2 Days
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_low_yesterday" /> Low 1 Days
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_high_yesterday" /> High 1 Days
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_sell_to_buy" /> Sell To Buy
                        </label>
                    </div>
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <div class="checkbox">
                        <label>
                            <input type="checkbox" id="is_buy_to_sell" /> Buy To Sell
                        </label>
                    </div>
                </div>
                <div class="clearfix"></div>
                <div class="form-group m-r-10 m-t-10">
                    <input type="text" name="available_amount" id="available_amount" placeholder="Available Amount" class="form-control input-sm" style="width:100px;" />
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <input type="text" name="profit_percentage" id="profit_percentage" value="1" placeholder="Profit%" class="form-control input-sm" style="width:100px;" />
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <input type="text" name="stoploss_percentage" id="stoploss_percentage" value="0.5" placeholder="StopLoss%" class="form-control input-sm" style="width:100px;" />
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <input type="text" name="ltp_from_percentage" id="ltp_from_percentage" placeholder="From %" class="form-control input-sm" style="width:100px;" />
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <input type="text" name="ltp_to_percentage" id="ltp_to_percentage" placeholder="To %" class="form-control input-sm" style="width:100px;" />
                </div>
                <div class="form-group m-r-10 m-t-10">
                    <button type="button" id="btn" class="btn btn-success btn-sm" data-bind="click:refresh">Refresh</button>
                </div>
            </div>
        </form>
    </div>
</div>
<div class="row" id="Company">
    <div class="col-md-12">
        <div class="row m-b-5">
            <div class="col-md-12">
                <div class="pull-right">
                    <button id="add" class="btn btn-sm btn-primary btn-add">Add</button>
                </div>
                <div class="pull-right m-r-15">
                    <button id="export" class="btn btn-sm btn-success btn-export">Export</button>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="pull-left m-r-15">
                    <span class="pull-left p-t-5 p-r-10">Rows</span>
                    <select id="rows" name="rows" class="pull-left form-control input-sm input-60">
                        <option value="10">10</option>
                        <option value="20" selected>20</option>
                        <option value="50">50</option>
                        <option value="200">200</option>
                        <option value="500">500</option>
                    </select>
                </div>
                <div class="pull-left">
                    <span id="paging_status" class="pull-left m-t-5"></span>
                </div>
            </div>
            <div class="col-md-6">
                <div class="pull-right">
                    <ul class="manual-pagination no-margin"></ul>
                </div>
            </div>
            <input type="hidden" name="page_index" value="1" />
            <input type="hidden" name="sort_name" value="ltp_percentage" />
            <input type="hidden" name="sort_order" value="desc" />
        </div>
        <table id="CompanyTable" class="table table-striped table-bordered table-hover table-condensed table-gray">
            <thead>
                <tr>
                    <th style="width: 50px">
                        &nbsp;
                    </th>
                    <th style="width: 50px" sortname="is_book_mark">
                        Mark
                    </th>
                    <th sortname="company_name">
                        Company
                    </th>
                    <th sortname="symbol">
                        Symbol
                    </th>
                    <th>
                        Category
                    </th>
                    <th sortname="ltp_percentage" class="text-right text-nowrap">
                        C%
                    </th>
                    <th class="text-right text-nowrap">
                        Quantity
                    </th>
                    <th sortname="ltp_price" class="text-right text-nowrap">
                        LTP
                    </th>
                    <th class="text-right text-nowrap">
                        BTS
                    </th>
                    <th class="text-right text-nowrap">
                        STB
                    </th>
                    <th class="text-right text-nowrap">
                        TR
                    </th>
                    <th class="text-right text-nowrap">
                        SL
                    </th>
                    <th sortname="open_price" class="text-right text-nowrap">
                        Open
                    </th>
                    <th sortname="low_price" class="text-right text-nowrap">
                        Low
                    </th>
                    <th sortname="high_price" class="text-right text-nowrap">
                        High
                    </th>
                    @*<th sortname="prev_percentage" class="text-right text-nowrap">
                            P%
                        </th>
                        <th sortname="high_percentage" class="text-right text-nowrap">
                            H%
                        </th>
                        <th sortname="low_percentage" class="text-right text-nowrap">
                            L%
                        </th>*@
                    <th sortname="high_count" class="text-right text-nowrap">
                        HC
                    </th>
                    <th sortname="low_count" class="text-right text-nowrap">
                        LC
                    </th>
                    @*<th class="text-right text-nowrap">
                            BTS Profit
                        </th>
                        <th class="text-right text-nowrap">
                            STB Profit
                        </th>*@
                    <th style="width: 50px">
                    </th>
                </tr>
            </thead>
            <tbody data-bind="foreach:rows">
                <tr>
                    <td class="cls-symbol">
                        <div style="width:5px;">
                            <a href="javascript:;" data-bind="attr:{'cid':id()}" class="tree-expand ex-plus pull-left">
                                <span>&nbsp;</span>
                            </a>
                        </div>
                    </td>
                    <td style="text-align:center;">
                        <div style="width:40px;">
                            <div class="pull-left">
                                <a href="javascript:;" class="is-book-mark">
                                    <i data-bind="css:{'fa-bookmark fg-primary': cBool(is_book_mark())==true,'fa-bookmark-o': cBool(is_book_mark())==false}" class="fa fa-bookmark"></i>
                                </a>
                            </div>
                            <div class="pull-left m-l-15">
                                <a href="javascript:;" data-bind="attr:{'symbol': symbol()}" class="refresh-symbol"><i class="fa fa-refresh"></i></a>
                            </div><div class="clearfix"></div>
                        </div>
                    </td>
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit" data-bind="text:company_name">
                        <div style="width:150px;overflow:hidden;" data-bind="text:company_name">
                        </div>
                    </td>
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit" data-bind="text:symbol"></td>
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit">
                        <div style="width:150px;overflow:hidden;" data-bind="text:category_name">
                        </div>
                    </td>
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                        <div data-bind="text:formatPercentage(ltp_percentage())"></div>
                    </td>
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                        <div data-bind="text:formatNumber(quantity())"></div>
                    </td>
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                        <div data-bind="text:formatNumber(ltp_price())"></div>
                    </td>
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                        <div data-bind="text:formatNumber(bts_sell_target(),4)"></div>
                        <div data-bind="text:formatNumber(bts_stop_loss_target(),4)"></div>
                    </td>
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                        <div data-bind="text:formatNumber(stb_sell_target(),4)"></div>
                        <div data-bind="text:formatNumber(stb_stop_loss_target(),4)"></div>
                    </td>
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                        <div data-bind="text:formatNumber(target_price(),4)"></div>
                    </td>
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                        <div data-bind="text:formatNumber(stop_loss_price(),4)"></div>
                    </td>
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                        <div data-bind="text:formatNumber(open_price())"></div>
                    </td>
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                        <div data-bind="text:formatNumber(low_price())"></div>
                    </td>
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                        <div data-bind="text:formatNumber(high_price())"></div>
                    </td>
                    @*<td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                            <div data-bind="text:formatPercentage(prev_percentage())"></div>
                        </td>
                        <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                            <div data-bind="text:formatPercentage(high_percentage())"></div>
                        </td>
                        <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                            <div data-bind="text:formatPercentage(low_percentage())"></div>
                        </td>*@
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                        <div data-bind="text:formatNumber(high_count())"></div>
                    </td>
                    <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                        <div data-bind="text:formatNumber(low_count())"></div>
                    </td>
                    @*<td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                            <div data-bind="text:formatNumber(bts_target_profit(),4)"></div>
                            <div data-bind="text:formatNumber(bts_stop_loss_profit(),4)"></div>
                        </td>
                        <td style="vertical-align:top;" class="cursor-pointer btn-edit text-right">
                            <div data-bind="text:formatNumber(stb_target_profit(),4)"></div>
                            <div data-bind="text:formatNumber(stb_stop_loss_profit(),4)"></div>
                        </td>*@
                    <td style="text-align:center;white-space:nowrap;">
                        <div style="width:80px;">
                            <div class="pull-left">
                                <a data-bind="attr:{'href': 'https://www.google.com/search?q=NSE:'+symbol().replace('&','%26')}" target="_blank">G</a>
                            </div>
                            <div class="pull-left m-l-15">
                                <a data-bind="attr:{'href': 'https://www.google.com/finance?q=NSE:'+symbol().replace('&','%26')}" target="_blank">GF</a>
                            </div><div class="clearfix"></div>
                        </div>
                        <div class="clearfix"></div>
                    </td>
                </tr>
            </tbody>
        </table>
    </div>
</div>
<div class="modal-container">
</div>
<!-- /ko -->
<script type="text/html" id="modal-template">
    <div id="modal-${name}" class="modal {{if is_modal_full==true}}container{{/if}} {{if position=='top'}}modal-top{{/if}}" tabindex="-1" data-focus-on="input:first" data-width="800" style="display: none;">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title">${title}</h4>
        </div>
        <div class="modal-body">
            <div class="edit-cnt p-10">
                <div class="row">
                    <div class="col-md-12">
                        <form method="post" class="form-horizontal form-validate" role="form" onsubmit="return false" data-bind="formValidate:{}">
                            <div class="form-group">
                                <label class="col-md-4 control-label">
                                    Company Name
                                </label>
                                <div class="col-md-7">
                                    <input type="text" class="form-control input-sm" name="company_name" data-bind="value:company_name" placeholder="Company Name" data-rule-required="true" data-msg-required="Required" data-placement="bottom" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-4 control-label">
                                    Symbol
                                </label>
                                <div class="col-md-7">
                                    <input type="text" class="form-control input-sm" name="symbol" data-bind="value:symbol" placeholder="Symbol" data-rule-required="true" data-msg-required="Required" data-placement="bottom" />
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-md-4 control-label">
                                    Category
                                </label>
                                <div class="col-md-7">
                                    <input type="hidden" name="category_name" />
                                </div>
                            </div>
                            <div class="form-group">
                                <div class="col-md-offset-4 col-md-8">
                                    <input type="hidden" name="id" data-bind="value:id" />
                                    <button type="submit" id="save" class="btn btn-sm btn-primary" data-loading-text="Save">Save</button>
                                    <button type="button" class="btn btn-sm btn-danger m-l-15" data-dismiss="modal">Cancel</button>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</script>
<script type="text/html" id="detail-template">
    <table cellpadding="0" cellspacing="0" class="table table-condensed no-margin-bottom" style="width:700px;">
        <thead>
            <tr>
                <th>
                    Symbol
                </th>
                <th>
                    Date
                </th>
                <th>
                    Time
                </th>
                <th class="text-right">
                    Open
                </th>
                <th class="text-right">
                    LTP
                </th>
                <th class="text-right">
                    LTP%
                </th>
            </tr>
        </thead>
        <tbody>
            {{each(j,row) rows}}
            <tr>
                <td>${row.symbol}</td>
                <td>${formatDate(row.trade_date)}</td>
                <td>${row.time}</td>
                <td class="text-right">${formatNumber(row.open_price)}</td>
                <td class="text-right">${formatNumber(row.ltp_price)}</td>
                <td class="text-right">${formatPercentage(row.ltp_percentage)}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</script>