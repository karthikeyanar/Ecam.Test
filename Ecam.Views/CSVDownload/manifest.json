{
  "name": "Money Control CSV Download",
  "description": "Adds a print button to the browser.",
  "version": "1.2",
  "permissions": [
    "tabs",
    "windows",
    "http://localhost:6868/*",
    "https://www.moneycontrol.com/*",
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Print this page",
    "default_icon": "print_16x16.png"
  },
  "manifest_version": 2
}